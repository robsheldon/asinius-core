{
    "name": "asinius/asinius",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~5.3 | ^7.0 | ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Asinius\\": "src/Asinius/"
        },
        "files": ["src/Asinius/Asinius.php"],
        "exclude-from-classmap": ["src/Asinius/autoload.php"]
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "atoum/atoum": "4.0",
        "phpstan/phpstan": "^0.12.31"
    }
}
